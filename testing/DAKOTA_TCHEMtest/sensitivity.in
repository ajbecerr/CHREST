environment,
    tabular_data
method,
  sampling
    sample_type lhs
    samples = 100
    variance_based_decomp
variables,
  uniform_uncertain 5
    upper_bounds      1.4     1.8     3.1     2.4     1.4
    lower_bounds      0.6     1.0     2.3     1.6     0.6
    descriptors 'x1' 'x2' 'x3' 'x4' 'x5'
interface,
        fork
        analysis_drivers = 'python driver.py params.in results.out'
        parameters_file = 'params.in'
        results_file    = 'results.out'
        verbatim
responses,
	response_functions = 1
	no_gradients
	no_hessians
